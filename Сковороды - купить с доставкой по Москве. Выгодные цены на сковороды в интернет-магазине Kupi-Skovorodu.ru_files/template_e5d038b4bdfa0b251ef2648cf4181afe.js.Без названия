function setSectionCrumbs() {



    // var crumbs=$('.breadCrumbs').html();
    crumbs = window.curCrumbs;
    //console.log(crumbs);
    window.localStorage.setItem("skovoroda_crumbs_"+document.location.pathname,crumbs);
    //console.log("kastrula_crumbs_"+document.location.pathname,crumbs);
}

function crumbsFromStorage(endelement){
    var parser = document.createElement('a');
    parser.href = document.referrer;
    var pathname = parser.pathname;

    var refcorr = possible_referers.indexOf(pathname);
    if(refcorr != -1)
    {
        crumbs=window.localStorage.getItem("skovoroda_crumbs_"+pathname);
        if(crumbs)
        {

            console.log("Ð¡rumbs in storage!");

            console.log(crumbs);

            var newbread = $('.breadCrumbs').html(crumbs);
            console.log(newbread);
            console.log(endelement);

            newbread.append(endelement);//ÐÐ¾Ð±Ð°Ð²Ð»ÑÐµÐ¼ Ð² ÐºÐ¾Ð½ÐµÑ Ð½Ð°Ð²Ð¸Ð³Ð°ÑÐ¸Ð¸ Ð½Ð°Ð·Ð²Ð°Ð½Ð¸Ðµ ÑÐ»ÐµÐ¼ÐµÐ½ÑÐ°
        }
    }



    //else console.log(window.localStorage);
}
