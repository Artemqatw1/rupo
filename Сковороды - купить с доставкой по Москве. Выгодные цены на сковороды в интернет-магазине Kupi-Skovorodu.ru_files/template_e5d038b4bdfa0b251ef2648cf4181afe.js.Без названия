function clear_val(id){
    $(id).val('');
}

function hide_show(element,active){
    var el = $(element);
    if(el.css('display')=='none'){
        if(typeof active!== undefined)
            $(active).addClass('active');
        el.fadeIn();
    }else{
        if(typeof active!== undefined)
            $(active).removeClass('active');
        el.fadeOut();
    }
}



