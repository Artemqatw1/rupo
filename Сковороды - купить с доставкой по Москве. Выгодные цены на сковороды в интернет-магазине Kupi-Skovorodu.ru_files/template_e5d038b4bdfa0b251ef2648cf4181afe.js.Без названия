function addItem(id) {
//    yaCounter24049669.reachGoal('add_to_cart');
    $.getJSON('/ajax/buy.php?action=ADD2BASKET&id=' + id, function (data) {
        $.each(data, function (key, val) {
            $("#cartQuantity").html(val.result);
            $("#header_basket_small").append('<span class="buttonOrder">Ð¾ÑÐ¾ÑÐ¼Ð¸ÑÑ Ð·Ð°ÐºÐ°Ð·</span>');
            $("#cartPrice").html(val.summ);
            $("#nameElement").html(val.name);
            $("#countryElement").html(val.brand+" - "+val.country);
            $("#artElement").html(val.artnumber);
            $("#priceElement").html(val.price);
            $("#photoElement").attr("src",val.image);
            $.fancybox({
                'speedIn':500,
                'speedOut':300,
                'href':'#buyForm',
                padding:0,
                scrolling:'no',
                helpers: {
                    overlay: {
                        locked: false
                    }
                }
            });
            setTimeout(function () {
                $.fancybox.close();
            }, 2000);
        });

        //Yandex send statistic;
        if(typeof yaCounter25084529 !== "undefined"){
            yaCounter25084529.reachGoal('click_button_buy');
            //console.log('send Yandex');
        }
    });
}



