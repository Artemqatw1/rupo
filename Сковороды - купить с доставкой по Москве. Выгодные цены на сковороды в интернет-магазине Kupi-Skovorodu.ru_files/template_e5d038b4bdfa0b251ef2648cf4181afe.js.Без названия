//native cookie function
function native_deleteCookie(name) {
    setCookie(name, "", {
        expires: -1
    })
}
function native_ajax(url, callback)  {
    var xhr = new XMLHttpRequest();
    xhr.open('GET', url);
    xhr.onreadystatechange = function () {
        if (this.readyState == 4) {
            if (this.status == 200){
                if(callback){
                    callback(this.responseText);
                }
            }
        }
    };
    xhr.send(null);
}
function native_setCookie(name, value, options) {
    /*ÐÐ¿Ð¸ÑÐ°Ð½Ð¸Ðµ*/
    /*
     ÐÑÐ³ÑÐ¼ÐµÐ½ÑÑ:

     name
     Ð½Ð°Ð·Ð²Ð°Ð½Ð¸Ðµ cookie

     value
     Ð·Ð½Ð°ÑÐµÐ½Ð¸Ðµ cookie (ÑÑÑÐ¾ÐºÐ°)

     options
     ÐÐ±ÑÐµÐºÑ Ñ Ð´Ð¾Ð¿Ð¾Ð»Ð½Ð¸ÑÐµÐ»ÑÐ½ÑÐ¼Ð¸ ÑÐ²Ð¾Ð¹ÑÑÐ²Ð°Ð¼Ð¸ Ð´Ð»Ñ ÑÑÑÐ°Ð½Ð¾Ð²ÐºÐ¸ cookie:

     expires
     ÐÑÐµÐ¼Ñ Ð¸ÑÑÐµÑÐµÐ½Ð¸Ñ cookie. ÐÐ½ÑÐµÑÐ¿ÑÐµÑÐ¸ÑÑÐµÑÑÑ Ð¿Ð¾-ÑÐ°Ð·Ð½Ð¾Ð¼Ñ, Ð² Ð·Ð°Ð²Ð¸ÑÐ¸Ð¼Ð¾ÑÑÐ¸ Ð¾Ñ ÑÐ¸Ð¿Ð°:
     Ð§Ð¸ÑÐ»Ð¾ â ÐºÐ¾Ð»Ð¸ÑÐµÑÑÐ²Ð¾ ÑÐµÐºÑÐ½Ð´ Ð´Ð¾ Ð¸ÑÑÐµÑÐµÐ½Ð¸Ñ. ÐÐ°Ð¿ÑÐ¸Ð¼ÐµÑ, expires: 3600 â ÐºÑÐºÐ° Ð½Ð° ÑÐ°Ñ.
     ÐÐ±ÑÐµÐºÑ ÑÐ¸Ð¿Ð° Date â Ð´Ð°ÑÐ° Ð¸ÑÑÐµÑÐµÐ½Ð¸Ñ.
     ÐÑÐ»Ð¸ expires Ð² Ð¿ÑÐ¾ÑÐ»Ð¾Ð¼, ÑÐ¾ cookie Ð±ÑÐ´ÐµÑ ÑÐ´Ð°Ð»ÐµÐ½Ð¾.
     ÐÑÐ»Ð¸ expires Ð¾ÑÑÑÑÑÑÐ²ÑÐµÑ Ð¸Ð»Ð¸ 0, ÑÐ¾ cookie Ð±ÑÐ´ÐµÑ ÑÑÑÐ°Ð½Ð¾Ð²Ð»ÐµÐ½Ð¾ ÐºÐ°Ðº ÑÐµÑÑÐ¸Ð¾Ð½Ð½Ð¾Ðµ Ð¸ Ð¸ÑÑÐµÐ·Ð½ÐµÑ Ð¿ÑÐ¸ Ð·Ð°ÐºÑÑÑÐ¸Ð¸ Ð±ÑÐ°ÑÐ·ÐµÑÐ°.
     path
     ÐÑÑÑ Ð´Ð»Ñ cookie.

     domain
     ÐÐ¾Ð¼ÐµÐ½ Ð´Ð»Ñ cookie.

     secure
     ÐÑÐ»Ð¸ true, ÑÐ¾ Ð¿ÐµÑÐµÑÑÐ»Ð°ÑÑ cookie ÑÐ¾Ð»ÑÐºÐ¾ Ð¿Ð¾ Ð·Ð°ÑÐ¸ÑÐµÐ½Ð½Ð¾Ð¼Ñ ÑÐ¾ÐµÐ´Ð¸Ð½ÐµÐ½Ð¸Ñ.
     */
    options = options || {};
    var expires = options.expires;
    if (typeof expires == "number" && expires) {
        var d = new Date();
        d.setTime(d.getTime() + expires * 1000);
        expires = options.expires = d;
    }
    if (expires && expires.toUTCString) {
        options.expires = expires.toUTCString();
    }
    value = encodeURIComponent(value);
    var updatedCookie = name + "=" + value;
    for (var propName in options) {
        updatedCookie += "; " + propName;
        var propValue = options[propName];
        if (propValue !== true) {
            updatedCookie += "=" + propValue;
        }
    }
    document.cookie = updatedCookie;
}
function native_getCookie(name) {
    var matches = document.cookie.match(new RegExp(
        "(?:^|; )" + name.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)"
    ));
    return matches ? decodeURIComponent(matches[1]) : undefined;
}
