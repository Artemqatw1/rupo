; /* Start:"a:4:{s:4:"full";s:53:"/bitrix/templates/skovoroda/js/city.js?14931009871539";s:6:"source";s:38:"/bitrix/templates/skovoroda/js/city.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
function approve_city(city)
{

    city = city.replace('Ñ','Ðµ');

    $('.lineTop .city_baloon').hide();
    $.cookie('CITY_NAME',city,{ expires: 10000, path: '/' });
	if(city=='ÐÐ¾ÑÐºÐ²Ð°')
		$('#header_phone').text('8 (495) 540-48-35');
	else
		$('#header_phone').text('8 (800) 500-13-52');

    //ÐÐ»Ñ sale.order.ajax
    $('input[name=ORDER_PROP_6]').val(city);

    location.reload();

}
function select_city(city)
{
    $('#lineTop_cityname').text(city);
    $('.city_select li.active').removeClass('active');
    $('.city_select li').each(function(n,el){
        if($(el).text()===city)
            $(el).addClass('active');
    });
    approve_city(city);
    $.fancybox.close();

}

function select_city_input()
{
    var city=$('#city_box_input').val();
    if(!city)
    {
//        console.log("empty city",city);
        city = $('.city_select li.active').text();
    }
    if(!city)
    {
//        console.log("select empty too",city);
        alert("ÐÐµ Ð²Ð²ÐµÐ´ÐµÐ½Ð¾ Ð½Ð°Ð·Ð²Ð°Ð½Ð¸Ðµ Ð³Ð¾ÑÐ¾Ð´Ð°!!!");
        return;
    }
    select_city(city);

}

$(document).ready(function(){
    $('.lineTop .city_baloon .close').live("click",function(event){
        approve_city($('#lineTop_cityname').text());
        event.stopPropagation();
    });
    $('.lineTop .city_baloon').live('click',function(){
        $.fancybox({href:"#city_select",padding:0});
    });
    $('#lineTop_cityname_link').fancybox({padding:0});
    $('.city_select li').live('click',function(){
        select_city($(this).text());
    });
});
/* End */
